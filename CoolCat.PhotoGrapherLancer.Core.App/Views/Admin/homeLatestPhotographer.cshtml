@model IEnumerable<CoolCat.PhotoGrapherLancer.Core.Entities.PhotoGrapher.PhotoGrapher>
@{
    ViewBag.Title = "Homelatestclient";
    Layout = null;
}

<div class="col-md-10">

    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6  toppad">


        <div class="panel panel-info">
            <div class="panel-heading">
                <h3 class="panel-title">Latest PhotoGrapher</h3>
            </div>
            <div class="panel-body">
                <div class="row">



                    <div class=" col-md-12 col-lg-12 col-xs-6 ">
                        <table class="table table-user-information">
                            <tbody>
                                @foreach (var item in Model.Take(3))
                    {
                        <tr>
                            <td>PhotoGrapher Name:</td>
                            <td><a href="/Admin/Profiledetails/@Html.DisplayFor(modelItem =>item.PhotoGrapherId)">@Html.DisplayFor(modelItem => item.FullName)</a></td>
                        </tr>
        }

                            </tbody>
                        </table>


                    </div>
                </div>


            </div>
    </div>
</div>
    </div>

