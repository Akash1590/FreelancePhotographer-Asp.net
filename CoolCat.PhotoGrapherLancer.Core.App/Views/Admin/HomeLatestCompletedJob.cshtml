@model IEnumerable<CoolCat.PhotoGrapherLancer.Core.Entities.Client.JobApplied>
@{
    ViewBag.Title = "HomeLatestCompletedJob";
    Layout = null;
}

<div class="col-md-10">

    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6  toppad">


        <div class="panel panel-info">
            <div class="panel-heading">
                <h3 class="panel-title">Latest Completed Job</h3>
            </div>
            <div class="panel-body">
                <div class="row">



                    <div class=" col-md-12 col-lg-12 col-xs-6 ">
                        <table class="table table-user-information">
                            <tbody>
                                @foreach (var item in Model.Take(3))
                                {
                                    <tr>
                                        
                                        <td><a href="/Admin/clientprofiledetails/@Html.DisplayFor(modelItem =>item.ClientId)">@Html.DisplayFor(modelItem => item.ClientName)</a> Hired <a href="/Admin/Profiledetails/@Html.DisplayFor(modelItem =>item.PhotoGrapherID)">@Html.DisplayFor(modelItem => item.PhotographerName)</a> For @Html.DisplayFor(modelItem => item.Price) taka .Event-@Html.DisplayFor(modelItem => item.Type)</td>
                                    </tr>
                                }

                            </tbody>
                        </table>


                    </div>
                </div>


            </div>
        </div>
    </div>
</div>

